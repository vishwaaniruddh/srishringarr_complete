<script>
function round_amount(amount){

    var round_num = amount.slice(-2);

        if(round_num < 50 ){
            add_amount = 50 - round_num;
        }
        if(round_num > 50 ){
            add_amount = 100 - round_num;
        }
        if(round_num ==  00 || round_num==50){
            add_amount = 00;
        }

    new_amount = parseInt(amount) + parseInt(add_amount);
    return new_amount;
}




